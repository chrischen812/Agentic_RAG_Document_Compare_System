<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:financial="http://example.com/financial#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <!-- Ontology Declaration -->
  <owl:Ontology rdf:about="http://example.com/financial#"/>

  <!-- Classes -->
  <owl:Class rdf:about="http://example.com/financial#Document">
    <rdfs:label xml:lang="en">Financial Document</rdfs:label>
    <rdfs:comment xml:lang="en">Base class for all financial documents</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Asset">
    <rdfs:label xml:lang="en">Financial Asset</rdfs:label>
    <rdfs:comment xml:lang="en">Financial asset or investment</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Liability">
    <rdfs:label xml:lang="en">Financial Liability</rdfs:label>
    <rdfs:comment xml:lang="en">Financial liability or debt</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Income">
    <rdfs:label xml:lang="en">Income</rdfs:label>
    <rdfs:comment xml:lang="en">Revenue or income source</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Expense">
    <rdfs:label xml:lang="en">Expense</rdfs:label>
    <rdfs:comment xml:lang="en">Cost or expenditure</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Investment">
    <rdfs:label xml:lang="en">Investment</rdfs:label>
    <rdfs:comment xml:lang="en">Investment instrument or product</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Risk">
    <rdfs:label xml:lang="en">Financial Risk</rdfs:label>
    <rdfs:comment xml:lang="en">Financial risk factor</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Return">
    <rdfs:label xml:lang="en">Financial Return</rdfs:label>
    <rdfs:comment xml:lang="en">Investment return or yield</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Portfolio">
    <rdfs:label xml:lang="en">Investment Portfolio</rdfs:label>
    <rdfs:comment xml:lang="en">Collection of investments</rdfs:comment>
  </owl:Class>

  <!-- Document Types -->
  <owl:Class rdf:about="http://example.com/financial#FinancialStatement">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Document"/>
    <rdfs:label xml:lang="en">Financial Statement</rdfs:label>
    <rdfs:comment xml:lang="en">Financial statement document</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#IncomeStatement">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#FinancialStatement"/>
    <rdfs:label xml:lang="en">Income Statement</rdfs:label>
    <rdfs:comment xml:lang="en">Profit and loss statement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#BalanceSheet">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#FinancialStatement"/>
    <rdfs:label xml:lang="en">Balance Sheet</rdfs:label>
    <rdfs:comment xml:lang="en">Balance sheet statement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#CashFlowStatement">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#FinancialStatement"/>
    <rdfs:label xml:lang="en">Cash Flow Statement</rdfs:label>
    <rdfs:comment xml:lang="en">Cash flow analysis</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#InvestmentReport">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Document"/>
    <rdfs:label xml:lang="en">Investment Report</rdfs:label>
    <rdfs:comment xml:lang="en">Investment performance report</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Budget">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Document"/>
    <rdfs:label xml:lang="en">Budget</rdfs:label>
    <rdfs:comment xml:lang="en">Financial budget document</rdfs:comment>
  </owl:Class>

  <!-- Asset Types -->
  <owl:Class rdf:about="http://example.com/financial#Equity">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:label xml:lang="en">Equity</rdfs:label>
    <rdfs:comment xml:lang="en">Equity investment</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Bond">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:label xml:lang="en">Bond</rdfs:label>
    <rdfs:comment xml:lang="en">Fixed income bond</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#RealEstate">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:label xml:lang="en">Real Estate</rdfs:label>
    <rdfs:comment xml:lang="en">Real estate investment</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Commodity">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:label xml:lang="en">Commodity</rdfs:label>
    <rdfs:comment xml:lang="en">Commodity investment</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#Cash">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:label xml:lang="en">Cash</rdfs:label>
    <rdfs:comment xml:lang="en">Cash or cash equivalent</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#MutualFund">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Investment"/>
    <rdfs:label xml:lang="en">Mutual Fund</rdfs:label>
    <rdfs:comment xml:lang="en">Mutual fund investment</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/financial#ETF">
    <rdfs:subClassOf rdf:resource="http://example.com/financial#Investment"/>
    <rdfs:label xml:lang="en">Exchange Traded Fund</rdfs:label>
    <rdfs:comment xml:lang="en">ETF investment</rdfs:comment>
  </owl:Class>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:about="http://example.com/financial#hasAsset">
    <rdfs:domain rdf:resource="http://example.com/financial#Portfolio"/>
    <rdfs:range rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:label xml:lang="en">has asset</rdfs:label>
    <rdfs:comment xml:lang="en">Portfolio contains asset</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/financial#hasLiability">
    <rdfs:domain rdf:resource="http://example.com/financial#Document"/>
    <rdfs:range rdf:resource="http://example.com/financial#Liability"/>
    <rdfs:label xml:lang="en">has liability</rdfs:label>
    <rdfs:comment xml:lang="en">Entity has liability</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/financial#generatesIncome">
    <rdfs:domain rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:range rdf:resource="http://example.com/financial#Income"/>
    <rdfs:label xml:lang="en">generates income</rdfs:label>
    <rdfs:comment xml:lang="en">Asset produces income</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/financial#incursExpense">
    <rdfs:domain rdf:resource="http://example.com/financial#Document"/>
    <rdfs:range rdf:resource="http://example.com/financial#Expense"/>
    <rdfs:label xml:lang="en">incurs expense</rdfs:label>
    <rdfs:comment xml:lang="en">Entity has expense</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/financial#hasRisk">
    <rdfs:domain rdf:resource="http://example.com/financial#Investment"/>
    <rdfs:range rdf:resource="http://example.com/financial#Risk"/>
    <rdfs:label xml:lang="en">has risk</rdfs:label>
    <rdfs:comment xml:lang="en">Investment carries risk</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/financial#achievesReturn">
    <rdfs:domain rdf:resource="http://example.com/financial#Investment"/>
    <rdfs:range rdf:resource="http://example.com/financial#Return"/>
    <rdfs:label xml:lang="en">achieves return</rdfs:label>
    <rdfs:comment xml:lang="en">Investment produces return</rdfs:comment>
  </owl:ObjectProperty>

  <!-- Datatype Properties -->
  <owl:DatatypeProperty rdf:about="http://example.com/financial#marketValue">
    <rdfs:domain rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="en">market value</rdfs:label>
    <rdfs:comment xml:lang="en">Current market value</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/financial#acquisitionCost">
    <rdfs:domain rdf:resource="http://example.com/financial#Asset"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="en">acquisition cost</rdfs:label>
    <rdfs:comment xml:lang="en">Original purchase price</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/financial#annualReturn">
    <rdfs:domain rdf:resource="http://example.com/financial#Investment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="en">annual return</rdfs:label>
    <rdfs:comment xml:lang="en">Annual return percentage</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/financial#riskRating">
    <rdfs:domain rdf:resource="http://example.com/financial#Investment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">risk rating</rdfs:label>
    <rdfs:comment xml:lang="en">Risk assessment rating</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/financial#maturityDate">
    <rdfs:domain rdf:resource="http://example.com/financial#Investment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">maturity date</rdfs:label>
    <rdfs:comment xml:lang="en">Investment maturity date</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/financial#dividendYield">
    <rdfs:domain rdf:resource="http://example.com/financial#Equity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="en">dividend yield</rdfs:label>
    <rdfs:comment xml:lang="en">Annual dividend yield</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/financial#couponRate">
    <rdfs:domain rdf:resource="http://example.com/financial#Bond"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="en">coupon rate</rdfs:label>
    <rdfs:comment xml:lang="en">Bond coupon rate</rdfs:comment>
  </owl:DatatypeProperty>

</rdf:RDF>
