<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:legal="http://example.com/legal#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <!-- Ontology Declaration -->
  <owl:Ontology rdf:about="http://example.com/legal#"/>

  <!-- Classes -->
  <owl:Class rdf:about="http://example.com/legal#Document">
    <rdfs:label xml:lang="en">Legal Document</rdfs:label>
    <rdfs:comment xml:lang="en">Base class for all legal documents</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Contract">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Document"/>
    <rdfs:label xml:lang="en">Contract</rdfs:label>
    <rdfs:comment xml:lang="en">Legal contract or agreement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Clause">
    <rdfs:label xml:lang="en">Clause</rdfs:label>
    <rdfs:comment xml:lang="en">Contract clause or provision</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Party">
    <rdfs:label xml:lang="en">Legal Party</rdfs:label>
    <rdfs:comment xml:lang="en">Party to a legal agreement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Obligation">
    <rdfs:label xml:lang="en">Legal Obligation</rdfs:label>
    <rdfs:comment xml:lang="en">Legal duty or responsibility</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Right">
    <rdfs:label xml:lang="en">Legal Right</rdfs:label>
    <rdfs:comment xml:lang="en">Legal right or entitlement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Liability">
    <rdfs:label xml:lang="en">Legal Liability</rdfs:label>
    <rdfs:comment xml:lang="en">Legal liability or responsibility</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Term">
    <rdfs:label xml:lang="en">Contract Term</rdfs:label>
    <rdfs:comment xml:lang="en">Specific term or condition</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#Remedy">
    <rdfs:label xml:lang="en">Legal Remedy</rdfs:label>
    <rdfs:comment xml:lang="en">Legal remedy or recourse</rdfs:comment>
  </owl:Class>

  <!-- Contract Types -->
  <owl:Class rdf:about="http://example.com/legal#ServiceAgreement">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:label xml:lang="en">Service Agreement</rdfs:label>
    <rdfs:comment xml:lang="en">Service provision contract</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#EmploymentContract">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:label xml:lang="en">Employment Contract</rdfs:label>
    <rdfs:comment xml:lang="en">Employment agreement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#NonDisclosureAgreement">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:label xml:lang="en">Non-Disclosure Agreement</rdfs:label>
    <rdfs:comment xml:lang="en">Confidentiality agreement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#LicenseAgreement">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:label xml:lang="en">License Agreement</rdfs:label>
    <rdfs:comment xml:lang="en">Software or content license</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#TermsOfService">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:label xml:lang="en">Terms of Service</rdfs:label>
    <rdfs:comment xml:lang="en">Service terms and conditions</rdfs:comment>
  </owl:Class>

  <!-- Clause Types -->
  <owl:Class rdf:about="http://example.com/legal#TerminationClause">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:label xml:lang="en">Termination Clause</rdfs:label>
    <rdfs:comment xml:lang="en">Contract termination provisions</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#LiabilityClause">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:label xml:lang="en">Liability Clause</rdfs:label>
    <rdfs:comment xml:lang="en">Liability limitation or allocation</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#IndemnificationClause">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:label xml:lang="en">Indemnification Clause</rdfs:label>
    <rdfs:comment xml:lang="en">Indemnification provisions</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#DisputeResolutionClause">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:label xml:lang="en">Dispute Resolution Clause</rdfs:label>
    <rdfs:comment xml:lang="en">Dispute resolution mechanism</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.com/legal#GoverningLawClause">
    <rdfs:subClassOf rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:label xml:lang="en">Governing Law Clause</rdfs:label>
    <rdfs:comment xml:lang="en">Applicable law specification</rdfs:comment>
  </owl:Class>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:about="http://example.com/legal#hasClause">
    <rdfs:domain rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:range rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:label xml:lang="en">has clause</rdfs:label>
    <rdfs:comment xml:lang="en">Contract contains clause</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/legal#hasParty">
    <rdfs:domain rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:range rdf:resource="http://example.com/legal#Party"/>
    <rdfs:label xml:lang="en">has party</rdfs:label>
    <rdfs:comment xml:lang="en">Contract involves party</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/legal#imposesObligation">
    <rdfs:domain rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:range rdf:resource="http://example.com/legal#Obligation"/>
    <rdfs:label xml:lang="en">imposes obligation</rdfs:label>
    <rdfs:comment xml:lang="en">Clause creates obligation</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/legal#grantsRight">
    <rdfs:domain rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:range rdf:resource="http://example.com/legal#Right"/>
    <rdfs:label xml:lang="en">grants right</rdfs:label>
    <rdfs:comment xml:lang="en">Clause grants right</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/legal#allocatesLiability">
    <rdfs:domain rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:range rdf:resource="http://example.com/legal#Liability"/>
    <rdfs:label xml:lang="en">allocates liability</rdfs:label>
    <rdfs:comment xml:lang="en">Clause addresses liability</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.com/legal#providesRemedy">
    <rdfs:domain rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:range rdf:resource="http://example.com/legal#Remedy"/>
    <rdfs:label xml:lang="en">provides remedy</rdfs:label>
    <rdfs:comment xml:lang="en">Clause specifies remedy</rdfs:comment>
  </owl:ObjectProperty>

  <!-- Datatype Properties -->
  <owl:DatatypeProperty rdf:about="http://example.com/legal#effectiveDate">
    <rdfs:domain rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">effective date</rdfs:label>
    <rdfs:comment xml:lang="en">Contract effective date</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/legal#expirationDate">
    <rdfs:domain rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:label xml:lang="en">expiration date</rdfs:label>
    <rdfs:comment xml:lang="en">Contract expiration date</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/legal#contractValue">
    <rdfs:domain rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="en">contract value</rdfs:label>
    <rdfs:comment xml:lang="en">Monetary value of contract</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/legal#jurisdictionCode">
    <rdfs:domain rdf:resource="http://example.com/legal#Contract"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">jurisdiction code</rdfs:label>
    <rdfs:comment xml:lang="en">Legal jurisdiction identifier</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/legal#clauseText">
    <rdfs:domain rdf:resource="http://example.com/legal#Clause"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">clause text</rdfs:label>
    <rdfs:comment xml:lang="en">Full text of the clause</rdfs:comment>
  </owl:DatatypeProperty>

</rdf:RDF>
